{
  "name": "mastermind-db",
  "version": "0.0.6",
  "description": "A CLI to manage database structures and ORM integrations with precision.",
  "bin": {
    "mastermind-db": "dist/index.js"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "postbuild": "npm run copy-templates",
    "copy-templates": "cp -R src/templates dist/templates",
    "start": "NODE_ENV=development ts-node src/index.ts",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrii-devs/mastermind-db.git"
  },
  "homepage": "https://github.com/andrii-devs/mastermind-db#readme",
  "keywords": [
    "cli",
    "database",
    "orm",
    "mastermind-db"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "author": "Andrii Devs",
  "license": "ISC",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "figlet": "^1.8.0",
    "handlebars": "^4.7.8",
    "inquirer": "^12.1.0",
    "js-yaml": "^4.1.0",
    "kleur": "^4.1.5",
    "mysql2": "^3.11.5",
    "nanospinner": "^1.2.2",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "sequelize-cli": "6.2.0",
    "sequelize-typescript": "^2.1.6",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/figlet": "^1.7.0",
    "@types/fs-extra": "^11.0.4",
    "@types/handlebars": "^4.1.0",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "fs-extra": "^11.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
